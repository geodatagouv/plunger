    "hasha": "^3.0.0",
    "htmlparser2": "^3.9.2",
    "mime-types": "^2.1.18",
    "mississippi": "^3.0.0",
    "normalize-url": "^2.0.1",
    "read-chunk": "^2.1.0",
    "rfc2047": "^2.0.1",
    "rimraf": "^2.6.1",
    "type-is": "^1.6.16"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "codecov": "^3.0.0",
    "finalhandler": "^1.1.1",
    "nyc": "^11.6.0",
    "serve-static": "^1.13.2",
    "test-listen": "^1.1.0",
