{
  "name": "plunger",
  "version": "2.3.3",
  "description": "Powerful link analyzer",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/inspireteam/plunger"
  },
  "author": "Jérôme Desboeufs <jerome.desboeufs@data.gouv.fr>",
